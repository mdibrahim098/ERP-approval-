
<table class="table table-hover general-table">
    <thead>
        <tr>
            <th>SL</th>
            <th style="text-align:center">Revise Remarks</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="ReviseHistory in ReviseHistoryList">
            <td>
                {{$index+1}}.
            </td>
            <td>
                {{ReviseHistory}}
            </td>
        </tr>
    </tbody>
</table>
<p ng-show="(ReviseHistoryList).length == 0" style="color:red">Nothing found!</p>

<div class="loadmore">
    <div ng-show="loaderMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>

